{
  "name": "shibc-aito",
  "version": "2.0.0",
  "description": "AITO - Autonomous AI CEO System for Shiba Classic",
  "main": "dist/orchestrator/index.js",
  "scripts": {
    "dev": "tsx watch src/orchestrator/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/orchestrator/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "db:migrate": "tsx src/lib/db/migrate.ts",
    "db:seed": "tsx src/lib/db/seed.ts",
    "docker:rebuild": "bash scripts/rebuild-all.sh",
    "docker:rebuild:win": "scripts\\rebuild-all.bat",
    "docker:deploy:local": "docker compose --profile agents up -d --force-recreate",
    "docker:stop": "docker compose --profile agents down --remove-orphans",
    "docker:logs": "docker compose --profile agents logs -f",
    "docker:verify": "bash -c 'for a in ceo cmo cto cfo coo cco dao; do echo aito-$a:; docker inspect aito-$a --format \"{{range .Config.Env}}{{println .}}{{end}}\" | grep LLM_ROUTING; done'"
  },
  "dependencies": {
    "@chaindead/telegram-mcp": "^0.1.23",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "@octokit/rest": "^22.0.1",
    "dockerode": "^4.0.2",
    "ethers": "^6.16.0",
    "etherscan-mcp": "^1.0.4",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "sharp": "^0.34.5",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@google/genai": "^1.34.0",
    "@types/dockerode": "^3.3.23",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "eslint": "^8.56.0",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Shiba Classic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brunzendorf/SHIBC-AITO.git"
  }
}
